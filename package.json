{
  "name": "create-easyintegration-app",
  "version": "1.0.0",
  "description": "This is a template project to create easy integration app",
  "bin": {
    "create-easyintegration-app": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easyintegration-blockchain/create-easyintegration-app.git"
  },
  "keywords": [
    "easyintegration"
  ],
  "author": "Rahul Kiran Gaddam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easyintegration-blockchain/create-easyintegration-app/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shelljs": "^0.8.5",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "react-refresh": "0.11.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  }
}
